<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.valdir.podcastapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SobreActivity" android:label="Sobre"/>
        <activity android:name=".DownloadsActivity" android:label="Downloads"/>
        <activity android:name=".CategoriaActivity" android:label="Categoria"/>
        <activity android:name=".BuscarActivity" android:label="Buscar"/>
        <activity android:name=".AudioBooksActivity" android:label="AudioBooks"/>
        <activity android:name=".AllpodcastsActivity" android:label="Podcasts"></activity>
    </application>

</manifest>